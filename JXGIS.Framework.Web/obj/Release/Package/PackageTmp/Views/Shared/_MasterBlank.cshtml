@using System.Web.Optimization
@{
    if (!HttpContext.Current.IsDebuggingEnabled)
    {
        Bundles.Reference("gStyle.css", "global");
        Bundles.Reference("Extends/CommonJS", "global");
        Bundles.Reference("Extends/Components/Base", "global");
        Bundles.Reference("Extends/Components/Framework", "global");
    }
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="renderer" content="webkit" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta name="description" content="@ViewBag.Description" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="@ViewBag.Favicon" />
    <script>
        var _bl_ = "@ViewBag.BaseUrl";
    </script>

    <link href="@ViewBag.BaseUrl/References/antd/antd.min.css" rel="stylesheet" />
    <link href="@ViewBag.BaseUrl/References/easyui/themes/metro/easyui.css" rel="stylesheet" />

    <script src="@ViewBag.BaseUrl/References/easyui/jquery.min.js"></script>
    <script src="@ViewBag.BaseUrl/References/react/react.min.js"></script>
    <script src="@ViewBag.BaseUrl/References/react/react-dom.min.js"></script>
    <script src="@ViewBag.BaseUrl/References/antd/antd.min.js"></script>

    <script src="@ViewBag.BaseUrl/References/is.js"></script>
    <script src="@ViewBag.BaseUrl/References/easyui/jquery.min.js"></script>
    <script src="@ViewBag.BaseUrl/References/easyui/jquery.easyui.min.js"></script>
    <script src="@ViewBag.BaseUrl/References/easyui/easyui-lang-zh_CN.js"></script>
    @if (HttpContext.Current.IsDebuggingEnabled)
    {
        @Styles.Render("~/gStyle")
        @Scripts.Render("~/gScript")
    }
    else
    {
        @Bundles.RenderStylesheets("global")
        @Bundles.RenderScripts("global")
    }
</head>
<body>
    @RenderBody()
</body>
</html>
